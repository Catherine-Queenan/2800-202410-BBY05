<!--Template for inputting dates. Used on scheduling and
    calendar pages.-->
<div class="form-group my-2" id="<%=divId%>" <%=xDivAttributes%>>
    <label for="<%=dateId%>"><%=label%></label>
    <input class="<%=dateClass%>" type="date" id="<%=dateId%>" name="<%=dateName%>" <%=xDateAttributes%>>
</div>