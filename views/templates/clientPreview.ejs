<!--Template for homepage preview boxes, used for the client 
    homepages (logged in and not)-->
    <div class="col-12">
        <div class="row align-items-center gy-3 gy-md-0 gx-xl-5 <%= justify %>">
            <div class="col-9 col-sm-8 col-md-6 background-container-preview <%= prevClass %>" style="background-image: url('<%= imageUrl %>');">
                <div>
                    <h1 class="h1 mb-3"><%= title %></h1>
                    <p class="mb-4 karla-heavy"><%= content %></p>
                    <form method="get">
                    <button formaction="<%= actionOne %>" class="btn btn-main mx-2 mb-3 <%= classesOne %>" <%= attributesOne %>>
                        <%= contentOne %>
                    </button>
                    <button formaction="<%= actionTwo %>" class="btn btn-secondary mx-2 mb-1 <%= classesTwo %>" <%= attributesTwo %>>
                        <%= contentTwo %>
                    </button>
                    </form>
                </div>
            </div>
        </div>
    </div>

