<div class="card" style="width: 18rem;">
  <%- include('defaultingPic', {pic: dog.dogPic})%>
    <div class="card-body">
      <h5 class="card-title"><%=dog.dogName%></h5>
      <%-include('button', {action:'',xClasses:'',xAttributes:'onclick=showFullDog(dog)',content:'See More'})%>
    </div>
  </div>


<!-- Modal -->
<div class="modal" id="modal-<%=dog.dogName%>" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        ...
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>



  